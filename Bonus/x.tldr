				"text": "void    ft_draw_ray(t_data *data)\n{\n    t_coordinate ray;\n    t_coordinate step;\n    int pixel_count;\n\n        ray.x = data->player->pos_in_pixels.x * data->scale;\n        ray.y = data->player->pos_in_pixels.y * data->scale;\n        step.x = cos(data->ray->angle) * (data->ray->distance) * data->scale;\n        step.y = sin(data->ray->angle) * (data->ray->distance) * data->scale;\n        pixel_count = data->ray->distance * data->scale;\n        step.x /= data->ray->distance * data->scale;\n        step.y /= data->ray->distance * data->scale;\n        while (pixel_count >=0)\n        {\n            ft_mlx_put_pixel(data->mlx, ray.x, ray.y, 0xAAAAAAFF);\n            ray.x += step.x;\n            ray.y += step.y;;\n            --pixel_count;\n        }\n}",
